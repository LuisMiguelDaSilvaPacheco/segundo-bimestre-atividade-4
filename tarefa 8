programa
{
	
	funcao inicio()
	{

		cadeia senha

		enquanto(verdadeiro){

		escreva("Diga uma senha \n")
		leia(senha)

		se(senha == "12345" ou senha == "admin" ou senha == "senha" ou senha == "xuxa"){
		
		escreva("Senha insegura \n")
		
		}senao{
			
		escreva("Senha segura")
		pare

		}
		}
	}
}
